{
  "name": "solus-9bd5",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "postinstall": "remix setup node",
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix watch",
    "dev:server": "cross-env NODE_ENV=development ts-node server/index.js",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:mocks": "node --require ./mocks server/index.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "database": "fly ssh console -C database-cli"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@remix-run/express": "^1.4.3",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/serve": "^1.4.3",
    "@remix-run/server-runtime": "^1.4.3",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "fp-ts": "^2.12.1",
    "io-ts": "^2.2.16",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "tiny-invariant": "^1.2.0",
    "ts-node": "^10.7.0",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.2",
    "@types/node": "^17.0.33",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "binode": "^1.0.5",
    "c8": "^7.11.2",
    "cypress": "^9.6.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^3.2.0",
    "msw": "^0.39.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "prisma": "^3.14.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.24",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.11.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
